{
  "name": "react-sass-boilerplate",
  "version": "1.0.0",
  "description": "Frontend setup for react with sass and webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p --prod",
    "new": "node bin/cli.js new",
    "start": "node bin/server.js",
    "test": "mocha internals/test/.setup-test.js app/**/*.test.js",
    "test:single": "mocha internals/test/.setup-test.js",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "react",
    "frontend",
    "boilerplate"
  ],
  "author": "Daniel Karlsson",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.6.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.8.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.11.3",
    "postcss-loader": "^1.1.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
